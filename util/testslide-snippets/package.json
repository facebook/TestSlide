{
  "name": "testslide-snippets",
  "displayName": "TestSlide Snippets collection",
  "description": "Snippets for easy and quick TestSlide tests writing",
  "publisher": "testslide-snippets",
  "icon": "icon.png",
  "repository": {
    "type": "github",
    "url": "https://github.com/facebook/TestSlide"
  },
  "version": "2.7.1",
  "engines": {
    "vscode": "^1.40.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "python",
        "path": "./snippets/snippets.code-snippets"
      }
